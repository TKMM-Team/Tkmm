<?xml version="1.0" encoding="utf-8"?>
<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:ext="using:Tkmm.Markup"
             xmlns:vm="using:Tkmm.Wizard.Pages"
             x:DataType="vm:EmulatorNameInputPageContext"
             x:Class="Tkmm.Wizard.Pages.EmulatorNameInputPage">
    <StackPanel>
        <TextBlock Text="{ext:Locale SetupWizard_EmulatorNameInput_Description}"
                   Margin="0,0,0,10" />
        <TextBox Text="{Binding EmulatorName}"
                 Watermark="{ext:Locale SetupWizard_EmulatorNameInput_Watermark}" />
    </StackPanel>
</UserControl> 